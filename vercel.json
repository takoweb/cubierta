{
  "rewrites": [{
    "source": "/(.*)",
    "destination": "/index.html"
  }],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "headers": [{
    "source": "/(.*)",
    "headers": [{
      "key": "Cache-Control",
      "value": "s-maxage=1, stale-while-revalidate"
    }]
  }]
}